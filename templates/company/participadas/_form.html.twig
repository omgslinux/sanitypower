{{ form_start(form) }}
    <div class="row">&nbsp;</div>
    {% if form.owned is defined %}
        <div class="row">
            <div class="col-6">
            {{ form_row(form.owned) }}
            </div>
            <div class="col-3">
            {{ form_row(form.direct) }} %
            </div>
            <div class="col-3">
            {{ form_row(form.percent) }} %
            </div>
        </div>
    {% endif %}
    {% if parent is defined %}
    <div class="row">
        <div class="col-12">
        {{ form_row(form.batch) }}
        </div>
    </div>
    {% endif %}
    <button class="btn btn-info">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}
