{{ form_start(form) }}
    <div class="row">&nbsp;</div>
    <div class="row">
        <div class="col-8">
        {{ form_row(form.fullname) }}
        </div>
        <div class="col-4">
        {{ form_row(form.ShortName) }}
        </div>
    </div>
    <div class="row">
        <div class="col-2">
        {{ form_row(form.country)}}
        </div>
        <div class="col-5">
        {{ form_row(form.level)}}
        </div>
        <div class="col-3">
        {{ form_row(form.category) }}
        </div>
        <div class="col-2">
        {{ form_row(form.active)}}
        </div>
    </div>
    <div class="row">
        <div class="col-12">
        {{form_row(form.notes)}}
        </div>
    </div>
    <button class="btn btn-info">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
